#name flashing
```python
a="#    ###  #   # ###   #  #   # ###    ##   ###   #  #   #"
b="#     #    # #   #   # # ##  ##      #  #   #   # # ##  #"
c="#     #     #    #  #   ## # ## ###  ####   #  #   ## # #"
d="#     #    # #   #  ######  ###  ##  #  ##  #  ######  ##"
e="#### ###  #   # ### #   ##   # ## #   ##   ### #   ##   #"
import time 
import os 
op=0 
while op<120: 
    time.sleep(0.5) 
    i = os.system('cls') 
    time.sleep(0.3)
    print (a) 
    print (b) 
    print (c) 
    print (d) 
    print (e) 
    op=op+1 
```
